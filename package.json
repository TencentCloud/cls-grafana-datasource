{
  "name": "tencent-cls-grafana-datasource",
  "version": "1.0.0",
  "description": "Log Service datasource plugin for Grafana",
  "author": "MeetzhDing",
  "license": "Apache-2.0",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "prettier": "prettier --config .prettierrc.js --write 'src/**/*.{tsx,ts,jsx,js,css,vue}'",
    "eslint": "eslint -c .eslintrc --fix 'src/**/*.{tsx,ts,jsx,js,css,vue}'"
  },
  "engines": {
    "node": ">=12"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  },
  "dependencies": {
    "@grafana/runtime": "^7.3.6",
    "@grafana/data": "^7.3.6",
    "@grafana/toolkit": "^7.3.6",
    "@grafana/ui": "^7.3.6",
    "crypto-js": "^4.0.0",
    "dot-qs": "^0.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-eslint": "^10.1.0",
    "eslint": "7.16.0",
    "eslint-config-alloy": "3.8.2",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "node-sass": "^5.0.0",
    "prettier": "2.2.1",
    "redux-logger": "^3.0.6",
    "sass-loader": "^10.1.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css}": [
      "eslint -c .eslintrc --fix",
      "prettier --config .prettierrc.js --write"
    ]
  }
}
